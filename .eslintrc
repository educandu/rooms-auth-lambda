{
  "extends": ["eslint:recommended", "plugin:import/recommended"],
  "env": {
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": ["error", { "ignore": ["^vitest/config$"] }]
  }
}
